package com.te.multithreading;

public class MultiThreadDemo extends Thread{

	@Override
	public void run() {
		for (char i = 'a'; i <= 'z'; i++) {
			System.out.println(i+" "+Thread.currentThread().getName());
		}
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}
}
